{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "project": "tsconfig.json"
    },
    "rules": {
        "no-new": "off",
        "@typescript-eslint/comma-dangle": ["error", "always-multiline"]
    }
}
